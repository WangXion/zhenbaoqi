page{
    background-color: rgba(243, 243, 243, 1);
}
.tuijianStore{
    margin-top: 30rpx;
}
.store_list{
    width: 96%;
    margin: auto;
}
.tuijianStore_counte {
    width: 100%;
    height: 480rpx;
    background-color: white;
    border-radius: 20rpx;
    padding-top: 30rpx;
    margin-top: 20rpx;
}
.store_v {
    position: absolute;
    width: 30rpx;
    height: 30rpx;
    font-size: 26rpx;
    text-align: start;
    line-height: 30rpx;
    background-color: rgba(240, 134, 34, 1);
    color: white;
    font-style: italic;
    border-radius: 50%;
    bottom: -12rpx;
    right: -7rpx;
}
.guanzhus {
    width: 140rpx;
    height: 50rpx;
    font-size: 30rpx;
    text-align: center;
    line-height: 50rpx;
    border-radius: 25rpx;
}

.tuijianStore_counte_top {
    width: 94%;
    height: 120rpx;
    margin: auto;
    display: flex;
    justify-content: space-between;
    align-items: center;

}

.tuijianStore_counte_goods {
    width: 96%;
    height: 270rpx;
    margin: auto;
    overflow: auto;
    margin-top: 30rpx;
}

.tuijianStore_counte_goods_list {
    width: 100%;
    height: 270rpx;
}