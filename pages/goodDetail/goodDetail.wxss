    line-height: 30rpx;
    font-size: 24rpx;
    color: rgba(233, 59, 68, 1);
    background-color: rgba(235, 218, 79, 1);
    top: 10rpx;
    right: 7%;
}

.up {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background-color: rgba(97, 97, 97, 0.295);
    display: flex;
    align-items: flex-end;
    z-index: 1000000000;

}

.up_buy {
    width: 100%;
    height: 50%;
    background-color: white;
    position: relative;
}

.up_off {
    position: absolute;
    right: 30rpx;
    top: 30rpx;
}

.up_btn {
    width: 94%;
    height: 80rpx;
    position: absolute;
