    display: flex;
    align-items: center;
    justify-content: space-around;
    text-align: center;
}

.shuoming_text {
    width: 94%;
    margin: auto;
    font-size: 26rpx;
    padding-bottom: 30rpx;
}

.bottom {
    width: 100%;
    height: 120rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    background-color: white;
    display: flex;
    justify-content: space-around;
    align-items: center;
    border-top: 2px solid rgba(246, 246, 246, 1);
}

.bottom_right {
    width: 60%;
    height: 80rpx;
    position: relative;

}

.bottom_btn {
    width: 100%;
    height: 100%;
